<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:ECOMap"
             x:Class="ECOMap.CustomCell"
             Shell.TabBarIsVisible="False">
    <StackLayout Orientation="Horizontal" Padding="10" Spacing="0">
        <Label x:Name="PageNameLabel" Text ="{Binding Title}" FontSize="Medium" VerticalOptions="CenterAndExpand"
               FontAttributes="Italic" TextColor="Green" TextDecorations="Underline"/>
    </StackLayout>
</ContentView>
